<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= " #{@government.name}の試験情報" %>
     
       <% @exams.each do|exam| %>
            <li class="exam_list">
              <%= exam.category.name%>
            </li>
            <li class="exam_list">
              <%= exam.date %>
            </li>
            <li class="exam_list">
              <%= exam.age %>
            </li>
          <% end %>
    </div>
  </div>
</div>